{
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "version": 2,
  "settings": {
    "appearanceTools": false,
    "border": {
      "color": false,
      "radius": false,
      "style": false,
      "width": false
    },
    "layout": {
      "contentSize": "65rem",
      "wideSize": "72rem"
    },
    "spacing": {
      "blockGap": null,
      "padding": true,
      "margin": true,
      "units": ["px", "em", "rem", "vh", "vw", "%"]
    },
    "color": {
      "customDuotone": false,
      "customGradient": true,
      "defaultDuotone": false,
      "defaultGradients": false,
      "defaultPalette": false,
      "palette": [
        {
          "name": "Primary",
          "slug": "primary",
          "color": "#0D243E"
        },
        {
          "name": "Secondary",
          "slug": "secondary",
          "color": "#FFD000"
        },
        {
          "name": "Blue",
          "slug": "blue",
          "color": "#14074f"
        },
        {
          "name": "Red",
          "slug": "red",
          "color": "#ca1334"
        },
        {
          "name": "Orange",
          "slug": "orange",
          "color": "#ff5100"
        },
        {
          "name": "Yellow",
          "slug": "yellow",
          "color": "#ffa800"
        },
        {
          "name": "Black",
          "slug": "black",
          "color": "#000000"
        },
        {
          "name": "White",
          "slug": "white",
          "color": "#ffffff"
        }
      ]
    },
    "typography": {
      "customFontSize": false,
      "fontStyle": false,
      "fontWeight": false,
      "letterSpacing": false,
      "lineHeight": false,
      "textDecoration": false,
      "textTransform": false,
      "dropCap": false,
      "fontFamilies": [
        {
          "fontFamily": "Open Sans, sans-serif",
          "slug": "global"
        },
        {
          "fontFamily": "Teko, sans-serif",
          "slug": "highlight"
        }
      ],
      "fontSizes": [
				{
					"fluid": {
						"min": "0.875rem",
						"max": "1rem"
					},
					"size": "1rem",
					"slug": "small"
				},
				{
					"fluid": {
						"min": "1rem",
						"max": "1.25rem"
					},
					"size": "1.25rem",
					"slug": "medium"
				},
				{
					"fluid": {
						"min": "1.75rem",
						"max": "1.875rem"
					},
					"size": "1.875rem",
					"slug": "large"
				},
				{
					"fluid": {
						"min": "2rem",
						"max": "2.5rem"
					},
					"size": "2.5rem",
					"slug": "x-large"
				},
				{
					"fluid": {
						"min": "2.1875rem",
						"max": "3.75rem"
					},
					"size": "3.75rem",
					"slug": "xx-large"
				}
			],
      "fluid": true
    },
    "custom": {
      "spacing": {
        "baseline": "1rem"
      }
    },
    "blocks": {
      "core/buttons": {
        "typography": {
          "fontSizes": [],
          "fontFamilies": []
        }
      },
      "core/button": {
        "typography": {
          "fontSizes": [],
          "fontFamilies": []
        },
        "spacing": {
          "padding": false
        },
        "border": {
          "color": false,
          "radius": false,
          "style": false,
          "width": false
        },
        "color": {
          "background": "transparent",
          "text": "var(--wp--preset--color--white)"
        }
      }
    }
  },
  "styles": {
    "typography": {
      "fontFamily": "var(--wp--preset--font-family--global)",
      "fontSize": "var(--wp--preset--font-size--medium)",
      "lineHeight": "1.4"
    },
    "elements": {
			"button": {
        "typography": {
          "fontSize": "1.375rem",
          "textTransform": "uppercase",
          "fontWeight": "600"
        },
        "spacing": {
          "padding": {
            "top": "0.8125rem",
            "bottom": "0.8125rem",
            "left": "1.25rem",
            "right": "1.25rem"
          }
        },
				"border": {
					"radius": "0"
				},
				"color": {
					"background": "var(--wp--preset--color--secondary)",
					"text": "var(--wp--preset--color--white)"
				},
        ":visited": {
					"color": {
						"background": "var(--wp--preset--color--secondary)",
            "text": "var(--wp--preset--color--white)"
					}
				},
				":hover": {
					"color": {
						"background": "var(--wp--preset--color--secondary)",
						"text": "var(--wp--preset--color--white)"
					}
				},
				":focus": {
					"color": {
            "background": "var(--wp--preset--color--secondary)",
						"text": "var(--wp--preset--color--white)"
					}
				},
				":active": {
					"color": {
						"background": "var(--wp--preset--color--secondary)",
						"text": "var(--wp--preset--color--white)"
					}
				}

			},
      "h1": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--highlight)",
          "fontSize": "var(--wp--preset--font-size--xx-large)",
          "lineHeight": "1.2",
          "textTransform": "uppercase"
        }
      },
      "h2": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--highlight)",
          "fontSize": "var(--wp--preset--font-size--x-large)",
          "lineHeight": "1.3",
          "textTransform": "uppercase"
        }
      },
      "h3": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--highlight)",
          "fontSize": "var(--wp--preset--font-size--large)",
          "lineHeight": "1.3",
          "textTransform": "uppercase"
        }
      },
      "h4": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--highlight)",
          "textTransform": "uppercase"
        }
      },
      "h5": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--highlight)"
        }
      }
		},
    "blocks": {
      "core/cover": {
        "spacing": {
          "padding": {
            "top": "var(--wp--preset--spacing--80)",
            "bottom": "var(--wp--preset--spacing--80)"
          }
        }
      },
      "core/columns": {
        "spacing": {
          "margin": {
            "bottom": "0",
            "top": "0"
          }
        }
      },
      "core/column": {
        "spacing": {
          "padding": {
            "top": "var(--wp--preset--spacing--40)",
            "bottom": "var(--wp--preset--spacing--40)",
            "left": "var(--wp--preset--spacing--40)",
            "right": "var(--wp--preset--spacing--40)"
          }
        }
      },
      "core/buttons": {
        "typography": {
          "fontSizes": [],
          "fontFamilies": []
        },
        "color": {
          "text": "var(--wp--preset--color--white)"
        }
      }
    }
  }
}
