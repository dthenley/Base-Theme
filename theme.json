{
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "version": 2,
  "settings": {
    "appearanceTools": false,
    "border": {
      "color": false,
      "radius": false,
      "style": false,
      "width": false
    },
    "layout": {
      "contentSize": "62rem",
      "wideSize": "72rem"
    },
    "spacing": {
      "blockGap": null,
      "padding": true,
      "margin": true,
      "units": ["px", "em", "rem", "vh", "vw", "%"]
    },
    "color": {
      "customDuotone": false,
      "customGradient": false,
      "defaultDuotone": false,
      "defaultGradients": false,
      "defaultPalette": false,
      "palette": [
        {
          "name": "Primary",
          "slug": "primary",
          "color": "#b9273a"
        },
        {
          "name": "Secondary",
          "slug": "secondary",
          "color": "#cccccc"
        },
        {
          "name": "Blue",
          "slug": "blue",
          "color": "#14074f"
        },
        {
          "name": "Red",
          "slug": "red",
          "color": "#ca1334"
        },
        {
          "name": "Orange",
          "slug": "orange",
          "color": "#ff5100"
        },
        {
          "name": "Yellow",
          "slug": "yellow",
          "color": "#ffa800"
        },
        {
          "name": "Black",
          "slug": "black",
          "color": "#1c2833"
        },
        {
          "name": "White",
          "slug": "white",
          "color": "#ecf0f1"
        }
      ]
    },
    "typography": {
      "customFontSize": false,
      "fontStyle": false,
      "fontWeight": false,
      "letterSpacing": false,
      "lineHeight": false,
      "textDecoration": false,
      "textTransform": false,
      "dropCap": false,
      "fontFamilies": [
        {
          "fontFamily": "Open Sans, sans-serif",
          "slug": "global"
        },
        {
          "fontFamily": "Raleway, sans-serif",
          "slug": "highlight"
        }
      ],
      "fontSizes": [
				{
					"fluid": {
						"min": "0.875rem",
						"max": "1rem"
					},
					"size": "1rem",
					"slug": "small"
				},
				{
					"fluid": {
						"min": "1rem",
						"max": "1.125rem"
					},
					"size": "1.125rem",
					"slug": "medium"
				},
				{
					"fluid": {
						"min": "1.75rem",
						"max": "1.875rem"
					},
					"size": "1.875rem",
					"slug": "large"
				},
				{
					"fluid": {
						"min": "2rem",
						"max": "2.5rem"
					},
					"size": "2.5rem",
					"slug": "x-large"
				},
				{
					"fluid": {
						"min": "2.1875rem",
						"max": "3.75rem"
					},
					"size": "3.75rem",
					"slug": "xx-large"
				}
			],
      "fluid": true
    },
    "custom": {
      "spacing": {
        "baseline": "1rem"
      }
    }
  },
  "styles": {
    "typography": {
      "fontFamily": "var(--wp--preset--font-family--global)",
      "fontSize": "var(--wp--preset--font-size--medium)",
      "lineHeight": "1.4"
    },
    "elements": {
      "h1": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--highlight)",
          "fontSize": "var(--wp--preset--font-size--xx-large)",
          "lineHeight": "1.2"
        }
      },
      "h2": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--highlight)",
          "fontSize": "var(--wp--preset--font-size--x-large)",
          "lineHeight": "1.3"
        }
      },
      "h3": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--highlight)",
          "fontSize": "var(--wp--preset--font-size--large)",
          "lineHeight": "1.3"
        }
      },
      "h4": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--highlight)"
        }
      },
      "h5": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--highlight)"
        }
      }
    }
  }
}
