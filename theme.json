{
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "version": 2,
  "settings": {
    "appearanceTools": false,
    "border": {
      "color": false,
      "radius": false,
      "style": false,
      "width": false
    },
    "layout": {
      "contentSize": "62rem",
      "wideSize": "72rem"
    },
    "spacing": {
      "blockGap": null,
      "padding": true,
      "margin": true,
      "units": ["px", "em", "rem", "vh", "vw", "%"]
    },
    "color": {
      "customDuotone": false,
      "customGradient": false,
      "defaultDuotone": false,
      "defaultGradients": false,
      "defaultPalette": false,
      "palette": [
        {
          "name": "Primary",
          "slug": "primary",
          "color": "#b9273a"
        },
        {
          "name": "Secondary",
          "slug": "secondary",
          "color": "#cccccc"
        },
        {
          "name": "Blue",
          "slug": "blue",
          "color": "#14074f"
        },
        {
          "name": "Red",
          "slug": "red",
          "color": "#ca1334"
        },
        {
          "name": "Orange",
          "slug": "orange",
          "color": "#ff5100"
        },
        {
          "name": "Yellow",
          "slug": "yellow",
          "color": "#ffa800"
        },
        {
          "name": "Black",
          "slug": "black",
          "color": "#1c2833"
        },
        {
          "name": "White",
          "slug": "white",
          "color": "#ecf0f1"
        }
      ]
    },
    "typography": {
      "customFontSize": false,
      "fontStyle": false,
      "fontWeight": false,
      "letterSpacing": false,
      "lineHeight": false,
      "textDecoration": false,
      "textTransform": false,
      "dropCap": false,
      "fontFamilies": [
        {
          "fontFamily": "Open Sans, sans-serif",
          "slug": "global"
        },
        {
          "fontFamily": "Raleway, sans-serif",
          "slug": "highlight"
        }
      ],
      "fontSizes": [
        {
          "slug": "small",
          "size": "1rem",
          "name": "Small"
        },
        {
          "slug": "medium",
          "size": "1.25rem",
          "name": "Medium"
        },
        {
          "slug": "large",
          "size": "1.9375rem",
          "name": "Large"
        },
        {
          "slug": "extra-large",
          "size": "2.4375rem",
          "name": "Extra Large"
        }
      ],
      "fluid": true
    },
    "custom": {
      "spacing": {
        "baseline": "1rem"
      }
    }
  },
  "styles": {
    "typography": {
      "fontFamily": "var(--wp--preset--font-family--global)",
      "fontSize": "var(--wp--preset--font-size--medium)",
      "lineHeight": "1.4"
    },
    "elements": {
      "h1": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--highlight)",
          "fontSize": "clamp(2.1875rem, 4vw, 4.35rem)",
          "lineHeight": "1.2"
        }
      },
      "h2": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--highlight)",
          "fontSize": "clamp(2rem, 3.25vw, 3.125rem)"
        }
      },
      "h3": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--highlight)"
        }
      },
      "h4": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--highlight)"
        }
      },
      "h5": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--highlight)"
        }
      }
    }
  }
}
